<div id="form_create">
    <h3 class="text-center">REGISTRO DE USUARIO</h3>
    <div class="form-group" id="container-data">
        <div class="col-md-12">
            <div class="form-group col-md-3">
                <label>Nombre *</label>
                <input class="form-control may" id="inp_nombre" type="text" autocomplete="off" />
            </div>
            <div class="form-group col-md-3">
                <label>Documento *</label>
                <input class="form-control may" id="inp_documento" type="text" autocomplete="off" />
            </div>
            <div class="form-group col-md-3">
                <label>Correo *</label>
                <input class="form-control may" id="inp_correo" type="email" autocomplete="off" />
            </div>
            <div class="form-group col-md-3">
                <label>Fecha Nacimiento * </label>
                <input class="form-control may" id="inp_fecha" type="date" autocomplete="off" />
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group col-md-3">
                <label>Contraseña *</label>
                <input class="form-control" id="inp_password" type="password" autocomplete="off" />
                <span id="val_password"></span>
            </div>
            <div class="form-group col-md-3">
                <label class="letter">Confirmar Contraseña *</label>
                <input class="form-control" id="inp_password2" type="password" autocomplete="off" />
                
            </div>
            <div class="form-group col-md-3">
                <label>Genero *</label>
                <select class="form-control" id="sel_genero">
                    <option>Masculino</option>
                    <option>Femenino</option>
                    <option>Otro</option>
                </select>
            </div>
        </div>       
        <div>
            <button id="btn_create" class="btn btn-success center-block" onclick="CreateUser()">CREAR</button>

            <span id="men_obligatorios"></span>
        </div>
    </div>
    
</div>
<script src="~/Scripts/BetProject/Usuario.js" type="text/javascript"></script>

<style>
    h3 {
        font-family: Papyrus, fantasy;
        font-weight:bold;
    }
</style>